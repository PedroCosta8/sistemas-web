# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET   /               controllers.HomeController.index
GET   /olamundo       controllers.HomeController.olaMundo
GET   /somaPost       controllers.HomeController.somaPost
POST  /formSomaPost   controllers.HomeController.formSomaPost
GET   /maior2Post     controllers.HomeController.maior2Post
POST  /maior2         controllers.HomeController.maior2
GET   /maior5Post     controllers.HomeController.maior5Post
POST  /maior5         controllers.HomeController.maior5
GET   /vogaisPost     controllers.HomeController.vogaisPost
POST  /vogais         controllers.HomeController.vogais
GET   /imparPost      controllers.HomeController.imparPost
POST  /impar          controllers.HomeController.impar

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
